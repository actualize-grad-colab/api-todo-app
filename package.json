{
  "name": "api-todo-app",
  "version": "1.0.0",
  "scripts": {
    "start": "node --env-file=./config/local.env -r ts-node/register ./src/main.ts",
    "watch": "nodemon --watch \"src/**\" --ext \"ts,json\"  --exec \"node --env-file=./config/local.env -r ts-node/register ./src/main.ts\"",
    "db:up": "dbmate --url=postgres://postgres:badpassword@localhost:5432/todo_app?sslmode=disable up",
    "db:seed": "psql -U postgres --dbname=postgres://postgres:badpassword@localhost:5432/todo_app?sslmode=disable -f ./db/seeds/dev.data.sql",
    "db:drop": "psql -U postgres --dbname=postgres://postgres:badpassword@localhost:5432/todo_app?sslmode=disable -f ./db/util/drop_tables.sql",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@types/pg": "^8.11.6",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "node-pg-format": "^1.3.5",
    "pg": "^8.12.0",
    "save": "^2.9.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.4.0",
    "@types/cors": "^2.8.17",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^4.17.21",
    "@types/node": "^20.13.0",
    "dbmate": "^2.17.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.2",
    "prettier-plugin-sql": "^0.18.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.11.0"
  }
}
